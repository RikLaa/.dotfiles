#configurations
alias zshconfig="vim ~/.zshrc"
alias vimrc="vim ~/.vimrc"
alias aliases="vim ~/.aliases"
alias gitconfig="vim ~/.gitconfig"
alias dotfiles="cd ~/.dotfiles && ls -al"

#file system
alias l='ls -al'
alias ..="cd .."

#other
alias php-server='php -S localhost:8000' 
alias dirs='dirs -v'





